{
    "version": 2,
    "builds": [
      {
        "src": "manage.py",
        "use": "@vercel/python"
      },
      {
        "src": "LOCATION_TRACKING_APP/wsgi.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "15mb"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "LOCATION/wsgi.py"
      }
    ]
  }
  